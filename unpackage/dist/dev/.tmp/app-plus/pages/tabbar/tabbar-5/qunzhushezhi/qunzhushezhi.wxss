page {
  background-color: #eee;
}
.title {
  height: 100rpx;
  line-height: 100rpx;
  color: #666;
  margin-left: 30rpx;
}
.ulBox {
  border-top: 1rpx solid #eee;
}
.ulBox ._li {
    background-color: #fff;
    height: 150rpx;
    padding: 0 30rpx;
    border-bottom: 1rpx solid #eee;
    line-height: 120rpx;
}
.ulBox ._li .imageBox {
      margin-top: 20rpx;
      width: 110rpx;
      height: 110rpx;
      display: inline-block;
      border-radius: 5px;
      background-color: #eee;
      overflow: hidden;
      position: relative;
      vertical-align: middle;
}
.ulBox ._li .imageBox image {
        box-sizing: border-box;
}
.ulBox ._li text {
      margin-left: 20rpx;
      vertical-align: sub;
}

